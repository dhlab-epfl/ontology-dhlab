        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#HostingCondition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The period during which something happens.</rdfs:comment>
        <rdfs:label xml:lang="en">period</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#periodization -->

