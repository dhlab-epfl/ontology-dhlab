<?xml version="1.0"?>
<rdf:RDF xmlns="http://vocab.dhlab.epfl.ch/garzoni#"
     xml:base="http://vocab.dhlab.epfl.ch/garzoni"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:grz="http://vocab.dhlab.epfl.ch/garzoni#">
    <owl:Ontology rdf:about="http://vocab.dhlab.epfl.ch/garzoni">
        <owl:versionIRI rdf:resource="http://vocab.dhlab.epfl.ch/garzoni"/>
        <rdfs:comment xml:lang="en">An ontology which models the information contained in the archival series &quot;Accordi dei Garzoni&quot; of the Venetian State Archives, relative to early modern venetian apprenticeship domain.</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#float">1.0</owl:versionInfo>
        <rdfs:label xml:lang="en">Garzoni Ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#conditionType -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#conditionType">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#Condition"/>
        <rdfs:comment xml:lang="en">This property allows to state the specific type of a  financial or hosting condition.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasCharge -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasCharge">
        <rdfs:label xml:lang="en">This property allows to assign a charge to a PersonMention or a PersonEntity.</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasCondition -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasCondition">
        <owl:inverseOf rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#isConditionOf"/>
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#Contract"/>
        <rdfs:range rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#Condition"/>
        <rdfs:label xml:lang="en">This property allows to assign a condition to a garzoni contract.</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasGeographicalOrigin -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasGeographicalOrigin">
        <rdfs:comment xml:lang="en">This property allows to specific the geographical origins of a PersonMention or PersonEntity.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasLocation -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasLocation">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#WorkshopMention"/>
        <rdfs:comment>This property allows to specify the location of an entity</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/place.owl"/>
        <rdfs:label xml:lang="en">hasLocation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasName -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasName">
        <rdfs:range rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#PersonAppellation"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">This property is used to attribute a PersonAppellation to a PersonMention or a PersonEntity.</rdfs:comment>
        <rdfs:label xml:lang="en">hasName</rdfs:label>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://www.cidoc-crm.org/html/5.0.4/cidoc-crm.html#P1</rdfs:seeAlso>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasNextPart -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasNextPart">
        <rdfs:comment xml:lang="en">This property allows to relate the different parts of a contract spanning over 2 pages.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasProfession -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasProfession">
        <rdfs:comment xml:lang="en">This property allows to state the profession of a PersonMention or PersonEntity.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasResidence -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasResidence">
        <rdfs:comment xml:lang="en">This property allows to specify the residence of a PersonMention or PersonEntity.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasRole -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasRole">
        <rdfs:isDefinedBy xml:lang="en">This property specifies the role of a PersonMention or PersonEntity.</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasWorkshop -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasWorkshop">
        <owl:inverseOf rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#isWorkshopOf"/>
        <rdfs:comment xml:lang="en">The relation between a PersonMention or PersonEntity and a Workshop.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#isConditionOf -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#isConditionOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#Condition"/>
        <rdfs:range rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#Contract"/>
        <rdfs:label xml:lang="en">isConditionOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#isNextPartOf -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#isNextPartOf">
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#Contract"/>
        <rdfs:range rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#Contract"/>
        <rdfs:label xml:lang="en">isNextPartOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#isWorkshopOf -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#isWorkshopOf">
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#WorkshopMention"/>
        <rdfs:label xml:lang="en">isWorkshopOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#placeType -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#placeType">
        <rdfs:label xml:lang="en">placeType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#relatedContract -->

    <owl:ObjectProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#relatedContract"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#ageText -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#ageText">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">To be deleted, only in MediaWiki</rdfs:comment>
        <rdfs:label xml:lang="en">ageText</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#applicationRule -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#applicationRule">
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#HostingCondition"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>none</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>sano</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>sano et infermo</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property allows to state the condition under which a rule is applied.</rdfs:comment>
        <rdfs:label xml:lang="en">applicationRule</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#clothingType -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#clothingType">
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#HostingCondition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">An indication on the type of clothes which is being paid to an apprentice.</rdfs:comment>
        <rdfs:label xml:lang="en">clothingType</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#currencyUnit -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#currencyUnit">
        <rdfs:comment xml:lang="en">To be deleted when harmonization with Wiki fully done.</rdfs:comment>
        <rdfs:label xml:lang="en">currencyUnit</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#denunciationDate -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#denunciationDate">
        <rdfs:comment xml:lang="en">This property allows to state at which date a flee has been denounced.</rdfs:comment>
        <rdfs:label xml:lang="en">denunciationDate</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#details -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#details">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This property allows to state additionnal details about whatever.</rdfs:comment>
        <rdfs:label xml:lang="en">details</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#hasMargin -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#hasMargin">
