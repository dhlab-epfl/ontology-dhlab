        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:comment xml:lang="en">This property allows to state whether a contract, in the register where it is written, has a complementary note in the margin or not.</rdfs:comment>
        <rdfs:label xml:lang="en">hasMargin</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#id -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#id">
        <rdfs:label xml:lang="en">id</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#identifiedPerson -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#identifiedPerson">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">To be deleted, only in MediaWiki.</rdfs:comment>
        <rdfs:label xml:lang="en">identifiedPerson</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#insigna -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#insigna">
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#WorkshopMention"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This property allows to state the emblem or &apos;name&apos; of a workshop.</rdfs:comment>
        <rdfs:label xml:lang="en">insigna</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#mediaWikiReference -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#mediaWikiReference">
        <rdfs:comment xml:lang="en">This property allows to link a contract to its corresponding object contract in the former MediaWiki database.</rdfs:comment>
        <rdfs:label xml:lang="en">mediaWikiReference</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#moneyInformation -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#moneyInformation">
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#FinancialCondition"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Additional information about money, if necessary.</rdfs:comment>
        <rdfs:label xml:lang="en">moneyInformation</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#oid -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#oid">
        <rdfs:domain rdf:resource="http://vocab.dhlab.epfl.ch/garzoni#Contract"/>
