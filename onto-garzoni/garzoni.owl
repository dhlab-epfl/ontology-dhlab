        <rdfs:label xml:lang="en">oid</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://vocab.dhlab.epfl.ch/garzoni#onMultiplePages -->

    <owl:DatatypeProperty rdf:about="http://vocab.dhlab.epfl.ch/garzoni#onMultiplePages">
